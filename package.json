{
	"name": "@condorhero/monorepo",
	"version": "0.1.5",
	"private": true,
	"packageManager": "pnpm@6.32.4",
	"description": "My Eslint config presets",
	"author": "CondorHero",
	"license": "MIT",
	"homepage": "https://github.com/condorheroblog/eslint-config-ape#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/condorheroblog/eslint-config-ape.git"
	},
	"bugs": {
		"url": "https://github.com/condorheroblog/eslint-config-ape/issues"
	},
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-config-preset"
	],
	"scripts": {
		"release": "bumpp package.json packages/*/package.json && pnpm -r publish",
		"preinstall": "npx only-allow pnpm",
		"lint:fix": "eslint . --fix",
		"lint": "eslint ."
	},
	"workspaces": [
		"packages/vue",
		"packages/react",
		"packages/basic",
		"packages/all",
		"packages/typescript"
	],
	"devDependencies": {
		"@condorhero/eslint-config": "workspace:^",
		"bumpp": "^8.2.1"
	}
}
